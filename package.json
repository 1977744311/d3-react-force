{
  "name": "d3-react-force",
  "version": "1.0.14-bate",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "start": "export NODE_ENV=development && webpack-serve --config ./webpack.config.js",
    "clean": "rimraf lib",
    "build": "npm run clean && echo Using Babel && babel --version && babel src/D3ReactForce --out-dir lib -s",
    "pub": "npm run build && npm publish",
    "release": "export NODE_ENV=production && webpack"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "babel-plugin-transform-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yacan8/d3-react-force.git"
  },
  "keywords": [
    "d3",
    "react"
  ],
  "author": "can.yang",
  "license": "ISC",
  "homepage": "https://github.com/yacan8/d3-react-force#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-serve": "^2.0.2"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "d3-drag": "^1.2.3",
    "d3-force": "^1.1.2",
    "d3-selection": "^1.3.2",
    "d3-zoom": "^1.7.3",
    "dagre": "^0.8.2"
  }
}
