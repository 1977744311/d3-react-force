{
  "name": "@td/d3-react-force",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "start": "export NODE_ENV=development && webpack-dev-server",
    "clean": "rimraf lib",
    "build": "npm run clean && echo Using Babel && babel --version && babel src/D3ReactForce --out-dir lib -s",
    "pub": "npm run build && npm publish",
    "release": "export NODE_ENV=production && webpack"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy",
      "babel-plugin-transform-class-properties",
      "babel-plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yacan8/d3-react-force.git"
  },
  "keywords": [
    "d3",
    "react"
  ],
  "author": "can.yang",
  "license": "ISC",
  "homepage": "https://github.com/yacan8/d3-react-force#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "react-hot-loader": "^4.0.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.21.2"
  },
  "peerDependencies": {
    "d3": ">=5.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
